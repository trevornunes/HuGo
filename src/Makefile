	@rm -f hugo$(EXEEXT)
	$(LINK) $(hugo_LDFLAGS) $(hugo_OBJECTS) $(hugo_LDADD) $(LIBS)
hugod$(EXEEXT): $(hugod_OBJECTS) $(hugod_DEPENDENCIES) 
